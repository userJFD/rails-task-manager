<!DOCTYPE html>
<html>
  <head>
    <title>Task List</title>
  </head>
  <body>
    <h1>Task List</h1>
      <ul>
        <% @tasks.each do |task| %>
          <li><%= link_to  task.title, task_path(task) %></li>
        <% end %>
      </ul>
    <%= link_to 'New Task', tasks_new_path %>
  </body>
</html>
